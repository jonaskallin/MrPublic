          },
          "editorMode": "code",
          "expr": "( sum(rate(hyperlens_spans_duration_seconds_bucket{le=\"1.5\"}[5m])) + sum(rate(hyperlens_spans_duration_seconds_bucket{le=\"3.0\"}[5m])) )/ 2 / sum(rate(hyperlens_spans_duration_seconds_count[5m]))",
          "format": "time_series",
          "legendFormat": "__auto",
          "range": true,
          "rawQuery": true,
          "rawSql": "WITH apdex_data AS (\n  SELECT\n    date_trunc('minute', t.startepoch) AS \"time\",\n    COUNT(*) FILTER (WHERE extract(EPOCH FROM t.endepoch - t.startepoch) <= 0.8) AS excellent_count,\n    COUNT(*) FILTER (WHERE extract(EPOCH FROM t.endepoch - t.startepoch) > 0.8 AND extract(EPOCH FROM t.endepoch - t.startepoch)  <= 1.5) AS good_count,\n    COUNT(*) FILTER (WHERE extract(EPOCH FROM t.endepoch - t.startepoch) > 1.5 AND extract(EPOCH FROM t.endepoch - t.startepoch)  <= 3) AS fair_count,\n    COUNT(*) FILTER (WHERE extract(EPOCH FROM t.endepoch - t.startepoch) > 3 AND extract(EPOCH FROM t.endepoch - t.startepoch)  <= 6) AS poor_count,\n    COUNT(*) FILTER (WHERE extract(EPOCH FROM t.endepoch - t.startepoch) > 6) AS unacceptable_count\n  FROM opentelemetry_spans t\n  WHERE t.trace->'instrumentationLibraryInfo'->>'name' = 'Request'-- timestamp BETWEEN 'start_date' AND 'end_date'  -- replace with your actual date range\n  GROUP BY \"time\"\n)\nselect\n  \"time\",\n  (excellent_count + good_count + (0.5 * fair_count)) / (excellent_count + good_count + fair_count + poor_count + unacceptable_count) AS apdex_score\nFROM apdex_data\nORDER BY \"time\" ASC;",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
